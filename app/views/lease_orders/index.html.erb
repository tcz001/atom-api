<p id="notice"><%= notice %></p>

<h1>Listing Lease Orders</h1>

<table>
  <thead>
    <tr>
      <th>Games</th>
      <th>Game versions</th>
      <th>Third partys</th>
      <th>Irb</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lease_orders.each do |lease_order| %>
      <tr>
        <td><%= lease_order.games %></td>
        <td><%= lease_order.game_versions %></td>
        <td><%= lease_order.third_partys %></td>
        <td><%= lease_order.irb %></td>
        <td><%= link_to 'Show', lease_order %></td>
        <td><%= link_to 'Edit', edit_lease_order_path(lease_order) %></td>
        <td><%= link_to 'Destroy', lease_order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lease order', new_lease_order_path %>
